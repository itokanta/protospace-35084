<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.name}さん", user_path(current_user.id), class: :greeting__link%>
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# <%= render partial: "prototype",locals:{ prototype: @prototype_only } %>
      <%# @prototype_only = Prototype.find(1)で一つのprototypeのみを表示する場合はrenderメソッドを上記に変更する %>
      <%= render partial: 'prototype', collection: @prototype %>
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    </div>
  </div>
</main>
